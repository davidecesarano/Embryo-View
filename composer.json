{
    "name": "davidecesarano/embryo-view",
    "type": "library",
    "description": "A PHP renderer for rendering PHP views in PSR response.",
    "keywords": [
        "view",
        "renderer",
        "psr"
    ],
    "homepage": "https://github.com/davidecesarano/embryo-view",
    "license": "MIT",
    "authors": [
        {
            "name": "Davide Cesarano",
            "homepage": "https://github.com/davidecesarano"
        }
    ],
    "require": {
        "php": ">=7.1",
        "davidecesarano/embryo-http": "dev-master",
        "davidecesarano/embryo-emitter": "dev-master",
        "psr/http-server-middleware": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Embryo\\": "Embryo/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.51"
    }
}
